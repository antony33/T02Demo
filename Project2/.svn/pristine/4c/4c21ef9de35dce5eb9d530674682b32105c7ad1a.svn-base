package p1;

public class Class1 {
	static int a;
	static int b;
	
	Class1(int x,int y){
		this.a=x;
		this.b=y;
	}

	
	public static void main(String[] args){
		Class1 obj=new Class1(80,2);   
		
		add();
		sub();
		mul();
		div();
		
	}
	
	public static void add(){
		
		int c = a + b;
		System.out.println(c+":");

	}

	public  static void  sub(){
		
		int c = a - b;
		System.out.println(c+":");
	}

	public static void mul(){
		
		int c = a * b;
		System.out.println(c+":");
	}

	public  static void div(){
		
		int c = a / b;
		System.out.println(c+":");
	}
	
	


}
